<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 配置别名 typeAliases和environment是平等级别的标签 type是实体类的完整名 alias是类的别名 -->
	<typeAliases>
		<typeAlias type="com.abchina.encodeAlgorithm.model.Algorithm" alias="Algorithm"/>
		<!-- 使用批量的方式 -->
	</typeAliases>
	
	<mappers>
		<!-- 映射文件的位置 -->
		<mapper resource="com/abchina/encodeAlgorithm/dao/xml/AlgorithmDao.xml" />
	</mappers>
</configuration>